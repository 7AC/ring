{
  "name": "@dgreif/ring-alarm",
  "version": "1.3.0",
  "description": "API for ring security alarm system",
  "main": "api/index.js",
  "scripts": {
    "test": "tslint **/*.ts",
    "lint": "tslint **/*.ts --fix",
    "build": "rm -rf lib && tsc --declaration",
    "pre-release": "npm run build && npm test",
    "release-api": "./build/publish-api.sh",
    "release-homebridge": "./build/publish-homebridge.sh",
    "release": "./build/publish-api.sh && ./build/publish-homebridge.sh",
    "patch": "npm run pre-release && npm version patch && npm run release",
    "minor": "npm run pre-release && npm version minor && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dgreif/ring-alarm.git"
  },
  "keywords": [
    "ring",
    "alarm",
    "api"
  ],
  "author": "dgreif",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "debug": "^4.1.1",
    "rxjs": "^6.4.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.0",
    "@types/node": "^10.12.24",
    "@types/socket.io-client": "^1.4.32",
    "dotenv": "^6.2.0",
    "prettier": "^1.16.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
